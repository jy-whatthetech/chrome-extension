{
  "name": "Google Drive Copy Utility",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Make multiple copies of items in Google Drive and mass modify them",
  "browser_action": {
    "default_icon": "images/icon.png",
    "default_title": "Google Drive Copy Utility",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon.png",
    "32": "images/icon.png",
    "48": "images/icon.png",
    "128": "images/icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://drive.google.com/*"
      ],
      "js": [
        "config.js",
        "content.js"
      ]
    }
  ],
  "background": {
    "persistent": false,
    "scripts": [
      "background.js"
    ]
  },
  "permissions": [
    "tabs",
    "activeTab",
    "storage",
    "https://ajax.googleapis.com/",
    "contentSettings",
    "identity",
    "https://www.googleapis.com/*"
  ],
  "key": "",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "Command+Shift+Y"
      },
      "description": "Opens filter toolbar"
    },
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+Q",
        "mac": "Command+Shift+Q"
      },
      "description": "Generates Ratings Preview"
    }
  }
}